MODULE user_command_0900 INPUT.
  save_ok = ok_code.

  CLEAR ok_code.
  CASE save_ok.
    WHEN 'BACK' OR 'EXIT' OR 'CANCEL' OR 'QX'.
      LEAVE TO SCREEN 0.
  ENDCASE.
ENDMODULE.
